{
  "parserConfig": {
    "postContainer": "table[data-post]",
    "selectors": {
      "authorName": "span.normalname a",
      "authorId": {
        "selector": "span.normalname a",
        "attribute": "href",
        "regex": "showuser=(\\d+)"
      },
      "creationDate": {
        "selector": "td.row2[width='99%']",
        "regex": "(\\d{2}\\.\\d{2}\\.\\d{2}, \\d{2}:\\d{2})"
      },
      "editDate": {
        "selector": "span.edit",
        "regex": "(\\d{2}\\.\\d{2}\\.\\d{2}, \\d{2}:\\d{2})"
      },
      "contentBody": "div.postcolor",
      "spoilers": "div.post-block.spoil",
      "spoilerTitle": ".block-title",
      "spoilerBody": ".block-body",
      "attachmentLink": "a.attach-file[href*=.txt]"
    }
  }
}